{
  "name": "echogrow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "turbo run build",
    "start": "pnpm --filter web start",
    "lint": "turbo run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit apps/web/src/__tests__ packages/ai-flows/__tests__",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "tests/*"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "lint": {},
      "test": {}
    }
  },
  "dependencies": {
    "@huggingface/inference": "^4.8.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/stripe": "^8.0.417",
    "huggingface": "^1.4.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "stripe": "^18.5.0"
  }
}
